{
  "name": "react-openlayers",
  "version": "0.3.1",
  "description": "OpenLayer React Components",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --port 9001 --content-base examples --config examples/webpack.config --open",
    "check-types": "tsc",
    "clean": "rimraf dist",
    "build": "npm-run-all --serial clean build:umd build:app",
    "build:umd": "cross-env NODE_ENV=production webpack",
    "build:app": "cross-env NODE_ENV=production webpack --config examples/webpack.config",
    "lint": "eslint --cache \"src/**/*.tsx\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenhwkim/react-openlayers.git"
  },
  "author": "Allen Kim<allenhwkim@gmail.com>",
  "bugs": {
    "url": "https://github.com/allenhwkim/react-openlayers/issues"
  },
  "homepage": "https://github.com/allenhwkim/react-openlayers#readme",
  "keywords": [
    "openlayer",
    "typescript",
    "react",
    "map"
  ],
  "devDependencies": {
    "@material-ui/core": "^3.7.1",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@types/google-maps": "^3.2.0",
    "@types/googlemaps": "^3.26.4",
    "@types/node": "^10.12.18",
    "@types/ol": "^5.3.3",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/parser": "^1.13.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "babel-loader": "^8.0.6",
    "babel-eslint": "10.x.x",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.2",
    "eslint": "5.x.x",
    "geo-coder": "^1.0.2",
    "html-loader": "^0.5.5",
    "npm-run-all": "^4.1.5",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-syntax-highlighter": "^10.1.2",
    "resolve-url-loader": "^3.0.0",
    "rimraf": "^2.6.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "typescript": "^3.5.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^16.7.0"
  },
  "dependencies": {
    "ol": "^5.3.3"
  }
